<a class="helplink"  tal:attributes="href self/attributes/href" tal:content="self/attributes/helptext" />

name: mathcountssolution mathcountsdifficulty

name: mathcountsworksheet
<div class="worksheet-title" tal:content="self/title">Title</div>
<form>
<ol class="worksheet-problems itemize">
	<li tal:repeat="item self"  tal:content="item">	</li>
</ol>
