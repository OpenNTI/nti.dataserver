name: mathcountsproblem
<div class="problem">
	 <input class="answerblank" type="number" tal:attributes="id self/attributes/probnum" />
	 <a tal:attributes="name self/id"/>
	 <span tal:content="self"/>
</div>

name: mathcountsresult
<div class="result hidden" tal:content="self"></div>


