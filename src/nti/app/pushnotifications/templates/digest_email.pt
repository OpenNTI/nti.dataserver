	<body>
		<div metal:use-macro="macro:header">
		<div id="green-bar"></div>
		<div id="logo-bar">
			<img src="http://d2ixlfeu83tci.cloudfront.net/images/email_logo.png" width="177" height="25"
			alt="NextThought Logo" />
		</div>
		</div>

		<table cellspacing="0" cellpadding="0" border="0"
			style="color: #333; background: #fff; padding: 0; margin:
			0; width: 100%; font: 15px/1.25em 'Helvetica Neue', Arial,
			Helvetica;">
			<tbody>
				<tr width="100%">
					<td valign="top" align="left" style="background: #EEF0F1; font: 15px/1.25em 'Helvetica Neue', Arial, Helvetica;" >
						<table style="border: none; padding: 0 18px; margin: 50px auto; width: 500px;">
							<tbody>
<!--!
								<tr width="100%" height="60">
									<td valign="top" align="left" style="border-top-left-radius: 4px; border-top-right-radius: 4px; background: #27709B url() bottom left repeat-x; padding: 10px 18px; text-align: center;">
										<img height="40" width="125" src="" title="XXX" style="font-weight: bold; font-size: 18px; color: #fff; vertical-align: top;"> </td>
								</tr>
-->
								<tr width="100%">
									<td valign="top" align="left" style="border-bottom-left-radius: 4px; border-bottom-right-radius: 4px; background:#fff; padding: 18px;">
										<h1 style="font-size: 20px; margin: 0;"> Notifications </h1>
										<hr style="clear:both;height:1px;border:0;border:none;width:100%;background:#DCDCDC;color:#DCDCDC;margin:18px 0;padding:0"/>
										<!--! <h3 style="font-size: 18px; margin: 18px 0;">On Site</a></h3> -->
										<div>
											<div style="margin-bottom: 18px;font: 15px/1.25em 'Helvetica Neue', Arial, Helvetica;"
												 tal:condition="options/discussion"
												 tal:define="notable options/discussion">
												<span style="margin: 0 2px 0 0;">
													<img src=""
														 alt="David Sabatini"
														 title="David Sabatini"
														 width="18" height="18"
														 style="display: inline;vertical-align:top;"
														 tal:attributes="alt notable/creator; title notable/creator; src notable/creator_avatar_url"
														 />
												</span>

												<strong tal:content="notable/creator">David Sabatini</strong> started a discussion
												<a href="" tal:attributes="href notable/href" tal:content="notable/title">4.3 Water, Weather, and Climate</a>
												in <a href="" tal:attributes="href notable/__parent__/href" tal:content="notable/__parent__/display_name">Open Discussions</a>.
												<div tal:condition="notable/remaining">
													Plus there ${python:request.localizer.pluralize('is', 'are', notable.remaining)} <tal:block tal:content="notable/remaining">6</tal:block> other
													${python:request.localizer.pluralize('discussion', 'discussions', notable.remaining)}.
												</div>
											</div>
											<div style="margin-bottom: 18px;font: 15px/1.25em 'Helvetica Neue', Arial, Helvetica;"
												 tal:condition="options/note"
												 tal:define="notable options/note">
												<span style="margin: 0 2px 0 0;">
													<img src=""
														 alt="David Sabatini"
														 title="David Sabatini"
														 width="18" height="18"
														 style="display: inline;vertical-align:top;"
														 tal:attributes="alt notable/creator; title notable/creator; src notable/creator_avatar_url"
														 />
												</span>

												<strong tal:content="notable/creator">Greg Higgins</strong>
												<!--! Sometimes there is no title, so we can't make that the link -->
												<a href="" tal:attributes="href notable/href">shared a note</a> <em tal:content="notable/display_name|notable/snippet">Test save</em>
												<span tal:condition="notable/__parent__/note_container_display_name">
												in <a href=""
													  tal:attributes="href notable/__parent__/note_container_href|default"
													  tal:content="notable/__parent__/note_container_display_name|default">a book</a>
												</span>.

												<div tal:condition="notable/remaining">
													Plus there ${python:request.localizer.pluralize('is', 'are', notable.remaining)} <tal:block tal:content="notable/remaining">6</tal:block> other
													${python:request.localizer.pluralize('note', 'notes', notable.remaining)}.
												</div>
											</div>
											<div style="margin-bottom: 18px;font: 15px/1.25em 'Helvetica Neue', Arial, Helvetica;"
												 tal:condition="options/comment"
												 tal:define="notable options/comment">
												<span style="margin: 0 2px 0 0;">
													<img src=""
														 alt="David Sabatini"
														 title="David Sabatini"
														 width="18" height="18"
														 style="display: inline;vertical-align:top;"
														 tal:attributes="alt notable/creator; title notable/creator; src notable/creator_avatar_url"
														 />
												</span>

												<strong tal:content="notable/creator">David Foster</strong>
												<a href="" tal:attributes="href notable/href">commented</a>
												<em tal:content="notable/snippet">&#8220;Hi Bart, Thanks for doing the research...&#8221;</em>
												in <a href="" tal:attributes="href notable/__parent__/href" tal:content="notable/__parent__/display_name">Open 9.4 Interpretation Essay</a>

												<div tal:condition="notable/remaining">
													Plus there ${python:request.localizer.pluralize('is', 'are', notable.remaining)} <tal:block tal:content="notable/remaining">6</tal:block> other
													${python:request.localizer.pluralize('comment', 'comments', notable.remaining)}.
												</div>
											</div>
											<div style="margin-bottom: 18px;font: 15px/1.25em 'Helvetica Neue', Arial, Helvetica;"
												 tal:condition="options/feedback"
												 tal:define="notable options/feedback">
												<span style="margin: 0 2px 0 0;">
													<img src=""
														 alt="David Sabatini"
														 title="David Sabatini"
														 width="18" height="18"
														 style="display: inline;vertical-align:top;"
														 tal:attributes="alt notable/creator; title notable/creator; src notable/creator_avatar_url"
														 />
												</span>

												<strong tal:content="notable/creator">David Foster</strong> left feedback
												<em tal:content="notable/snippet">&#8220;Ii Bart, Thanks for doing the research...&#8221;</em>
												in <a href="" tal:attributes="href notable/href" tal:content="notable/assignment_name">Open 9.4 Interpretation Essay</a>

												<div tal:condition="notable/remaining">
													Plus there ${python:request.localizer.pluralize('is', 'are', notable.remaining)} <tal:block tal:content="notable/remaining">6</tal:block> other feedback
													${python:request.localizer.pluralize('item', 'items', notable.remaining)}.
												</div>
											</div>
											<div style="margin-bottom: 18px;font: 15px/1.25em 'Helvetica Neue', Arial, Helvetica;"
												 tal:condition="options/circled"
												 tal:define="notable options/circled">
												<span style="margin: 0 2px 0 0;">
													<img src=""
														 alt="David Sabatini"
														 title="David Sabatini"
														 width="18" height="18"
														 style="display: inline;vertical-align:top;"
														 tal:attributes="alt notable/creator; title notable/creator; src notable/creator_avatar_url"
														 />
												</span>

												<tal:block tal:condition="notable/remaining">
													<strong tal:content="notable/creator">David Foster</strong> and
													<tal:block tal:content="notable/remaining">6</tal:block> others added you to a group.
												</tal:block>
												<tal:block tal:condition="not:notable/remaining">
													<strong tal:content="notable/creator">David Foster</strong> added you to a group.
												</tal:block>
											</div>
											<div style="margin-bottom: 18px;font: 15px/1.25em 'Helvetica Neue', Arial, Helvetica;"
												 tal:condition="options/grade"
												 tal:define="notable options/grade">
												<span style="margin: 0 2px 0 0;">
													<img src=""
														 alt="David Sabatini"
														 title="David Sabatini"
														 width="18" height="18"
														 style="display: inline;vertical-align:top;"
														 tal:attributes="alt notable/creator; title notable/creator; src notable/creator_avatar_url"
														 />
												</span>

												<strong tal:content="notable/creator">David Foster</strong> assigned a grade
												in <a href="" tal:attributes="href notable/href" tal:content="notable/assignment_name|notable/course/display_name|default">a course</a>.

												<div tal:condition="notable/remaining">
													Plus there ${python:request.localizer.pluralize('is', 'are', notable.remaining)} <tal:block tal:content="notable/remaining">6</tal:block> other
													${python:request.localizer.pluralize('grade', 'grades', notable.remaining)}.
												</div>
											</div>
										</div>

								</td>
								</tr>
								<tr width="100%">
									<td valign="top" align="left" style="padding: 16px;font-size: 12px; line-height: 1.25em;">
										<p style="color: #999; margin-bottom: 8px;">
											This message was sent to <span tal:content="options/email_to">jamadden@gmail.com</span>.
											If you don't want to receive these emails in the future, please
											<a tal:attributes="href options/unsubscribe_link"
											   href="" style="color: #666;">unsubscribe</a>.
										</p>
									</td>

								</tr>
								</tbody>
